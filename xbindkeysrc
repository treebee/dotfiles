"amixer set Master 2dB-"
XF86AudioLowerVolume

"amixer set Master toggle"
XF86AudioMute

"amixer set Master 2dB+"
XF86AudioRaiseVolume

"mpc prev"
XF86AudioPrev

"mpc toggle"
XF86AudioPlay

"mpc next"
XF86AudioNext

"uzbl --uri mail.zoho.com"
XF86Mail
